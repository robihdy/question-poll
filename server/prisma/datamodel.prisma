type Question {
  id: ID! @id
  createdAt: DateTime! @createdAt
  description: String!
  votes: Int!
}

type Event {
  id: ID! @id
  createdAt: DateTime! @createdAt
  name: String!
  code: String! @unique
  postedBy: Organizer
}

type Organizer {
  id: ID! @id
  name: String!
  email: String! @unique
  password: String!
  events: [Event!]!
}